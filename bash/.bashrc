# If not running interactively, don't do anything
[[ $- != *i* ]] && return

# Aliases
alias ls='lsd --color=auto'
alias grep='grep --color=auto'
alias xdd='xxd'
alias cat='bat --pager never --style=plain'

# Command Prompt Line
PS1='[\u@\h \W]\$ '


# Starship Init
eval "$(starship init bash)"

# Rye
source "$HOME/.rye/env"


